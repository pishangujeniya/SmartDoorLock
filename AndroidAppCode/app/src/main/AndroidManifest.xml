<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pishangujeniya.SmartDoorLock">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:name="com.pishangujeniya.SmartDoorLock.Globalshare"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pishangujeniya.SmartDoorLock.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pishangujeniya.SmartDoorLock.Connect" />
        <activity android:name="com.pishangujeniya.SmartDoorLock.BlueList" />
        <activity android:name="com.pishangujeniya.SmartDoorLock.StatusGet" />
        <activity android:name="com.pishangujeniya.SmartDoorLock.ToLock" />
        <activity android:name="com.pishangujeniya.SmartDoorLock.ToUnlock" />
        <activity android:name="com.pishangujeniya.SmartDoorLock.History"></activity>
    </application>

</manifest>